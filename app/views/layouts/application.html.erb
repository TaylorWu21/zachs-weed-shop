<!DOCTYPE html>
<html>
  <head>
    <title>ZachsWeedShop</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script src="https://use.fontawesome.com/0dcef984ef.js"></script>

    <meta charset='utf-8', name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no">
  </head>

  <body>
    <%= render(partial: 'layouts/navbar') %>
    <%= yield %>
    <%= render(partial: 'layouts/footer') %>
  </body>
</html>
